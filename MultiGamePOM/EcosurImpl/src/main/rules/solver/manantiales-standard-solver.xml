    <scoreCalculator>
        <scoreCalculatorType>SIMPLE</scoreCalculatorType>
    </scoreCalculator>
    <selector>
        <selector>
            <moveFactoryClass>mx.ecosur.multigame.solver.manantiales.RotateMoveFactory</moveFactoryClass>
             <relativeSelection>0.99</relativeSelection>
        </selector>
        <selector>
            <moveFactoryClass>mx.ecosur.multigame.solver.manantiales.SwapMoveFactory</moveFactoryClass>
            <relativeSelection>0.99</relativeSelection>
        </selector>        
